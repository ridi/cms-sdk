{
  "name": "ridibooks/cms-sdk",
  "description": "Ridibooks CMS SDK",
  "type": "library",
  "license": "MIT",
  "require": {
    "apache/thrift": "^0.10.0",
    "firebase/php-jwt": "^4.0.0",
    "twig/twig": "^2.0",
    "symfony/http-foundation": "^4.3|^5.0.7"
  },
  "suggest": {
    "laravel/lumen-framework": "Needed to use LumenApplication",
    "rcrowe/twigbridge": "Needed to use Lumen with Twig"
  },
  "autoload": {
    "psr-4": {
      "Ridibooks\\Cms\\": "lib/php/src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "scripts": {
    "test": "phpunit lib/php"
  }
}
